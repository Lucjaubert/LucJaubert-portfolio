{
    "extends": "./tsconfig.json",
    "compilerOptions": {
      "outDir": "./out-tsc/server",
      "target": "ES2022",
      "module": "ES2022",
      "types": ["node"],
      "useDefineForClassFields": false,
      "noUnusedLocals": false
    },
    "files": ["server.ts", "src/main.server.ts"],
    "exclude": [
      "src/test.ts",
      "src/**/*.spec.ts",
      "src/main.ts"
    ]
  }
  